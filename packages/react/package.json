{
  "name": "@haro/jpform-react",
  "version": "0.1.0",
  "description": "React hooks for Japanese business forms",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "keywords": [
    "japanese",
    "form",
    "react",
    "hooks",
    "postal"
  ],
  "author": "Hideki Matsuoka",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/haro/jpform.git",
    "directory": "packages/react"
  },
  "dependencies": {
    "@haro/jpform-core": "workspace:*",
    "@haro/jpform-postal": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.48",
    "react": "^18.2.0",
    "rimraf": "^5.0.5"
  }
}
